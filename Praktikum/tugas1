# print(pers(1,2,3))
def jumlah(a,b):
    return -b/a
def kali(a,c):
    return c/a
def hasil(a,b,c):
    return (((jumlah(a,b))**2)-(2*kali(a,c)))
print (hasil(1,0,-4))